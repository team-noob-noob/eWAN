@model eWAN.Identity.Controllers.Account.RegisterViewModel

<partial name="_ValidationSummary" />

<form asp-route="Register">
    <div class="form-group">
        <label asp-for="Username">Username</label>
        <input type="text" placeholder="Username" asp-for="Username"/>
    </div>
    
    <div class="form-group">
        <label asp-for="Password">Password</label>
        <input type="password" placeholder="Password" asp-for="Password"/>
    </div>

    <div class="form-group">
        <label asp-for="FirstName">First Name</label>
        <input type="text" placeholder="Juan" asp-for="FirstName"/>
    </div>

    <div class="form-group">
        <label asp-for="MiddleName">Middle Name</label>
        <input type="text" placeholder="Topacio" asp-for="MiddleName"/>
    </div>

    <div>
        <label asp-for="LastName">Last Name</label>
        <input type="text" placeholder="Dela Cruz" asp-for="LastName"/>
    </div>

    <div>
        <label asp-for="HomeAddress">Home Address</label>
        <input type="text" placeholder="Imus, Cavite" asp-for="HomeAddress"/>
    </div>

    <div>
        <label asp-for="Email">Email</label>
        <input type="email" placeholder="test@test.com" asp-for="Email"/>
    </div>

    <input type="submit" />
</form>
